{
  "name": "sarehub/servitiom",
  "description": "generic microservices manager",
  "keywords": [
    "docker",
    "microservice",
    "manager"
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "type": "project",
  "readme": "./README.md",
  "license": "MPL-2.0",
  "authors": [
    {
      "name": "Andrzej Wasiak",
      "email": "mararok@gmail.com"
    }
