    "mockery/mockery": "1.1.*",
    "guzzlehttp/guzzle": "^6"
  },
  "require": {
    "php": "^7.1",
    "sarehub/microt": "0.8.*",
    "sarehub/commons": "0.7.*",
    "sarehub/dockerutil": "0.3.*",
    "sarehub/microorm": "dev-version_0.2",
    "doctrine/orm": "2.6.*",
    "respect/validation": "^1.1",
    "tuupola/slim-basic-auth": "^2.2",
    "php-di/php-di": "^6.0",
